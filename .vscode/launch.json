{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "Iniciar Server y WebApi",
      "configurations": ["Iniciar Server", "Iniciar WebApi"]
    }
  ],
  "configurations": [
    {
      "name": "Iniciar Server",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/01_Client/Web/Server/bin/Debug/net6.0/Server.dll",
      "args": [],
      "cwd": "${workspaceFolder}/01_Client/Web/Server",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Iniciar WebApi",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/02_Server/Api/WebApi/bin/Debug/net6.0/WebApi.dll",
      "args": [],
      "cwd": "${workspaceFolder}/02_Server/Api/WebApi",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
